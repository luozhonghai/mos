<h1>期間・季節・数量限定メニュー </h1>

<div class="contents">
<ul class="menuList">
  <% @products.each do |product| %>
    <li>
      <%= link_to image_tag(product.photo), product %>
      <%= link_to product.name, product %>
      <br>
      <%= "¥"+product.price.to_s %>
    </li>
  <% end %>
</ul>
</div>
